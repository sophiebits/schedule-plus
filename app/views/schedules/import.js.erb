var data = 
<%= 
@schedule.to_json(:include => {
  :scheduled_courses => {:include => 
    {
      :course => {}, 
      :lecture => {:include => :lecture_section_times}, 
      :recitation => {:include => :recitation_section_times}
    }
  }
}).html_safe 
%>;

$('#login').fadeOut();
$('#add-schedule input[type=submit]')
  .css('background-image',"url(/images/form-add.png)");
$('.main-aside .tooltip').hide();
$('#start-page').animate({ left:'-100%' });
$('#main-bg').animate({ left:'0%' });
$('#main-page').animate({ left:'0%' }).queue(function() {
  $('#main-page').css('position','relative');

  addSchedule(data.user.scheduled_courses);
  
  $('.main-aside .tooltip').delay(data.user.scheduled_courses.length * 200).fadeIn(800);

});
